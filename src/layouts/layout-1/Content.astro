---
import "./styles.css"
import { dataContent } from "../../data/data.json";
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="description" content="Astro description" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="generator" content={Astro.generator} />
  </head>
  <tite></tite>
  <body>
    <div id="content-wrapper">
      {
        dataContent.map((item) => (
          <div id="main-wrapper">
            <div class="img-content">
              <a href={`/blog/${item.slug}`}>
                <img src={item.imageSrc} alt="áº£nh content" />
              </a>
            </div>
            <div class="post-info">
              <h2 class="post-title">
                <a href="">{item.postTitle}</a>
              </h2>
              <div class="post-meta">
                <span>
                  <a href="">{item.postCategory}</a>
                </span>
                <span>
                  <a href="">{item.author}</a>
                </span>
                <span>
                  <a href="">{item.date}</a>
                </span>
              </div>
              <p class="post-snippet">{item.postSnippet}</p>
              <a href={`/blog/${item.slug}`} class="read-more">
